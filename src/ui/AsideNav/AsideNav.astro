---
import { cva, type VariantProps } from "class-variance-authority";

interface Props extends VariantProps<typeof tempClasses> {
  title: string;
}

const tempClasses = cva([], {
  variants: {},
});

const { title } = Astro.props;
---

<nav class:list={["p-3"]}>
  <h2 class:list={["font-semibold"]}>
    {title}
  </h2>
  <slot />
</nav>
